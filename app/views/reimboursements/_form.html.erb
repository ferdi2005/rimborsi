<% end %>
  <div class="form-group mb-3">
          <%= form.label :state_id %>
      <%= form.collection_select :state_id, State.all, :id, :name, { prompt: true }, { class: "form-select" }  %>
      </div>
  <div class="form-group mb-3">
          <%= form.label :user_id %>
