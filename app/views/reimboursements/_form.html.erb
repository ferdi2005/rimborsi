      </div>
  <div class="form-group mb-3">
          <%= form.label :bank_account_id %>
      <%= form.collection_select :bank_account_id, BankAccount.all, :id, :name, { prompt: true }, { class: "form-select" }  %>
      </div>
  <div class="form-group mb-3">
          <%= form.label :paypal_account_id %>
