<div class="container-fluid">
  <p id="notice"><%= notice %></p>
  <h1>Notes</h1>
  <table class="table">
    <thead>
      <tr>
                  <th>State</th>
                  <th>Reimboursment</th>
                  <th>User</th>
                  <th>Text</th>
                <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @notes.each do |note| %>
      <tr>
                  <td><%= note.state_id %></td>
                  <td><%= note.reimboursement_id %></td>
                  <td><%= note.user_id %></td>
                  <td><%= note.text %></td>
                <td><%= link_to 'Show', note %></td>
        <td><%= link_to 'Edit', edit_note_path(note) %></td>
        <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <br>
  <%= link_to 'New Note', new_note_path, class: "btn btn-primary" %>
</div>
